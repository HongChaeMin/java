import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		int a = sc.nextInt();
		int r = sc.nextInt();
		int n = sc.nextInt();

		int num = a;
		while(a<=n) {
			num *= r;
			a++;
			System.out.println("num : " + num);
			System.out.println("a : " + a);
		}
		
		System.out.println(num);
		
	}

}
