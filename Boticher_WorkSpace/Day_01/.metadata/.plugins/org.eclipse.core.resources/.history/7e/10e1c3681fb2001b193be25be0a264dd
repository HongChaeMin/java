package threadEx01;

public class ThreadImplements implements Runnable{

	private String resource;
	
	public ThreadImplements(String resource) {
		this.resource = resource;
	}
	
	@Override
	public void run() {
		for(int i=0; i<10; i++) {
			Thread.sleep(500); // 1000ÀÌ 1ÃÊÀÓ
			System.out.println(this.resource);
		}
	}

}
