package jenerics;

public class GenericsPerson<T, V> {

	private T name;
	private V age;

	public GenericsPerson(T name, V age) {
		this.name = name;
		this.age = age;
	}

	public static <T, V> void disp(GenericsPerson<T, V>p1, GenericsPerson<T, V>p2) {
		System.out.println(p1.name + " " + p1.age);
		System.out.println(p2.name + " " + p2.age);
	}

}
